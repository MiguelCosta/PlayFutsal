<table class="table">
  <thead>
    <tr>
      <th>Team</th>
      <th>Matches Played</th>
      <th>Goals</th>
    </tr>
  </thead>

  <tbody>
    <% group.teams.each_with_index do |team, i| %>
      <%= render :partial => 'play_futsal/teams/group_row', :locals => { :team => team } %>
    <% end %>
  </tbody>
</table>